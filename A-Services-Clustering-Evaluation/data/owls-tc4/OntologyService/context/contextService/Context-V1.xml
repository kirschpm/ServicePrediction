<?xml version="1.0" encoding="UTF-8"?>
<ctx:context xmlns:ctx="http://www.citypassenger.com/services/ContextSchema.xsd" 
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
            xmlns:xs="http://www.w3.org/2001/XMLSchema"
            xsi:schemaLocation="http://www.citypassenger.com/services/ContextSchema.xsd">
  <ctx:condition>
    <ctx:contextElement>
      <ctx:hasEntity resource="http://www.citypassenger.com/services/ContextModel.owl#concept.Entity.Device"/>
      <ctx:hasScope resource="http://www.citypassenger.com/services/ContextModel.owl#concept.Scope.Resource"/>
      <ctx:hasRepresentation resource="http://www.citypassenger.com/services/ContextModel.owl#concept.Representation.Resource_Representation"/>
      <ctx:contextValueSet>
        <ctx:contextValue>
          <ctx:hasScope resource="http://www.citypassenger.com/services/ContextModel.owl#concept.Scope.Resource.Memory"/>
          <ctx:hasRepresentation resource="http://www.citypassenger.com/services/ContextModel.owl#concept.Representation.Resource_Representation.Memory_Representation"/>
          <ctx:valueSet>
        	<ctx:valueElement>
        		<ctx:operator resource="http://www.citypassenger.com/services/ContextModel.owl#Concept.Operator.Lower"/>
          		<ctx:value>100.5</ctx:value>
          	</ctx:valueElement>
          </ctx:valueSet>
       </ctx:contextValue> 
       <ctx:contextValue>
          <ctx:hasScope resource="http://www.citypassenger.com/services/ContextModel.owl#concept.Scope.Resource.Battery"/>
          <ctx:hasRepresentation resource="http://www.citypassenger.com/services/ContextModel.owl#concept.Representation.Resource_Representation.Battery_Representation"/>
          <ctx:valueSet>
        	<ctx:valueElement>
        		<ctx:operator resource="http://www.citypassenger.com/services/ContextModel.owl#Concept.Operator.Lower-Equal"/>
          		<ctx:value>50</ctx:value>
          	</ctx:valueElement>
          </ctx:valueSet>
       </ctx:contextValue>     
        <ctx:contextValue>
          <ctx:hasScope resource="http://www.citypassenger.com/services/ContextModel.owl#concept.Scope.Resource.Network"/>
          <ctx:hasRepresentation resource="http://www.citypassenger.com/services/ContextModel.owl#concept.Representation.Resource_Representation.Network_Representation"/>
          <ctx:valueSet>
        	<ctx:valueElement>
        		<ctx:operator resource="http://www.citypassenger.com/services/ContextModel.owl#Concept.Operator.Equal"/>
          		<ctx:value>wifi</ctx:value>
          	</ctx:valueElement>
          </ctx:valueSet>
       </ctx:contextValue>
      </ctx:contextValueSet>      
    </ctx:contextElement>
  </ctx:condition>
  
  <ctx:state>
    <ctx:contextElement>
      <ctx:hasEntity resource="http://tempuri.org"/>
      <ctx:hasScope resource="http://tempuri.org"/>
      <ctx:hasRepresentation resource="http://tempuri.org"/>
      <ctx:contextValueSet>
      <ctx:contextValue>
        <ctx:hasScope resource="http://tempuri.org"/>
        <ctx:hasRepresentation resource="http://tempuri.org"/>
        <ctx:valueSet>
        	<ctx:valueElement>
        		<ctx:operator resource="http://www.citypassenger.com/services/ContextModel.owl#Concept.Operator.Equal"/>
          		<ctx:value>ctx:value</ctx:value>
          	</ctx:valueElement>
        </ctx:valueSet>
      </ctx:contextValue>
      </ctx:contextValueSet>
    </ctx:contextElement>
  </ctx:state>
</ctx:context>